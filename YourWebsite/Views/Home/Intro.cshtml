@{
    ViewBag.Title = "Giới thiệu";
}
<style>
    p {
        font-size: 20px;
        line-height: 150%;
    }

    @@font-face {
        font-family: iCiel;
        src: url('/Content/font/Arimo-Regular.ttf');
